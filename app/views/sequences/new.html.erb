<%
  @title = :sequence_add_title.t
  tabs = [
    link_with_query(:cancel_and_show.t(type: :observation),
                    @observation.show_link_args)
  ]
  @tabsets = { right: draw_tab_set(tabs) }
%>

<%= render(partial: "observation_title",
           locals: { observation: @observation }) %>
<%= form_for(
      :sequence,
      url: add_query_param(sequence_path(id: @observation.id))
    ) do |form| %>
  <%= render(partial: "form", locals: { form: form }) %>
<% end %>
