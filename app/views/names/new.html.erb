<%
  @title ||= :create_name_title.t
  tabs = [
      link_to(:all_objects.t(type: :name), names_path)
  ]
  @tabsets = { right: draw_tab_set(tabs) }
%>

<%= form_for(:name, url: add_query_param(action: :create)) do |form| %>

  <%= render(partial: "names/form", locals: { form: form }) %>

  <div class="text-center">
    <%= submit_tag(:CREATE.l, class: "btn btn-default") %>
  </div>

<% end %>
