<%
  @title = :sequence_edit_title.t(name: @sequence.unique_format_name)
  tabs = [
    link_with_query(:cancel_and_show.t(type: :observation),
                    @sequence.observation.show_link_args)
  ]
  @tabsets = { right: draw_tab_set(tabs) }
%>

<%= form_for(:sequence, url: {action: :add_sequence}) do |form| %>
  <%= render(partial: "form_sequence", locals: {form: form}) %>
<% end %>
