<div class="list-group-item">
  <%= link_with_query(object.unique_format_name, object.show_link_args) %>
  <br>
  <%= link_with_query(object.observation.unique_format_name.t,
                  object.observation.show_link_args) %>
  <% if object.deposit? %>
    <br>
    <%= sequence_archive_link(object) %>:
    <%= sequence_accession_link(object) %>
  <% end %>
  <br>
  <%= content_tag(:small, object.created_at.web_time) %>:
  <%= user_link(object.user) %>
</div>
