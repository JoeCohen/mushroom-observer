// What to do when receiving a request to mark obs as reviewed/not

var checkbox = $("#mark_as_reviewed_<%= j @obs_id %>");
var label = checkbox.closest('label');

if("<%= @reviewed %>" == "true") {
  label.html("<%= j :marked_as_reviewed.t %>").prepend(checkbox);
  // console.log("reviewed <%= @reviewed %>");
} else if("<%= @reviewed %>" == "false"){
  label.html("<%= j :mark_as_reviewed.t %>").prepend(checkbox);
  // console.log("not reviewed <%= @reviewed %>");
}
