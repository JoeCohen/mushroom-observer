<%
  if @links && @links.any?
    tabs = @links.map {|str,url| link_to(str, url)}
    @tabsets = { right: draw_tab_set(tabs) }
  end

  flash_error(@error) if @error and (!@objects or @objects.empty?)
%>

<%= paginate_block(@pages) do %>
  <% if @objects != [] %>
    <table class="table table-striped" width="100%">
      <% for desc in @objects || [] %>
        <tr>
          <td><%= link_with_query(desc.format_name.t,
                    :action => 'show_name_description', :id => desc.id) %><td>
        </tr>
      <% end %>
    </table>
  <% end %>
<% end %>
