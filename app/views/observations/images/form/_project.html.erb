<%= fields_for(:project) do |f_p| %>
  <%= f_p.label("id_#{project.id}", link_to_object(project)) %>
  <%= f_p.check_box("id_#{project.id}",
                    checked: @project_checks[project.id],
                    disabled: @image.user != @user && !project.is_member?(@user),
                    class: "form-control") %>
<% end %>